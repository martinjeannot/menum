<?xml version='1.0' encoding='utf-8'?>
<widget id="com.geomenum.Menum" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.GeoBroker" />
    </feature>
    <feature name="AdmobPlugin">
        <param name="android-package" value="com.admob.cordova.plugin.AdmobAdPlugin" />
    </feature>
    <name>Menum</name>
    <description>
        The Geomenum mobile app.
    </description>
    <author email="contact@geomenum.com" href="http://geomenum.com/">
        Geomenum Team
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
